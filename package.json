    "react-helmet": "^6.1.0",
    "react-icons": "^4.11.0",
    "react-share": "^4.4.1",
    "react-twitter-widgets": "^1.11.0",
    "react-typography": "^0.16.23",
    "react-uuid": "^1.0.2",
    "typography": "^0.16.24",
    "typography-theme-stow-lake": "^0.16.19",
    "url-join": "^5.0.0"
