    "@atlaskit/tooltip": "^17.8.8",
    "@chakra-ui/gatsby-plugin": "^3.1.3",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@formspree/react": "^2.5.1",
    "dotenv": "^16.3.1",
    "framer-motion": "^10",
